{
  "buildCommand": "prisma generate && next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm ci",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["cle1"],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ]
}